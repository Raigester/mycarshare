# Secret key for Django (replace with a secure one in production)
SECRET_KEY=your_django_secret_key_here
# Enable/disable debug mode (should be False in production)
DEBUG=True
# Hosts allowed to access the app (comma-separated)
ALLOWED_HOSTS=127.0.0.1,localhost


# Secret key for signing JWT tokens (can be the same as SECRET_KEY or different)
SIGNING_KEY=your_jwt_signing_key_here


# Redis host (usually localhost for dev)
REDIS_HOST=localhost
# Redis port
REDIS_PORT=6379
# Redis DB number (usually 0)
REDIS_DB=0


# LiqPay API keys
LIQPAY_PUBLIC_KEY=your_liqpay_public_key
LIQPAY_PRIVATE_KEY=your_liqpay_private_key


# Database configuration
DB_NAME=carsharing_db
# Database user
DB_USER=carsharing_user
# Database password
DB_PASSWORD=your_strong_password
# Database host
DB_HOST=db
# Database port
DB_PORT=5432
